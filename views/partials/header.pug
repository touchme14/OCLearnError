.main-header
  // Logo Header
  .logo-header(data-background-color='blue')
    a.logo(href='index.html')
      img.navbar-brand(src='../assets/img/logo.svg' alt='navbar brand')
    button.navbar-toggler.sidenav-toggler.ml-auto(type='button' data-toggle='collapse' data-target='collapse' aria-expanded='false' aria-label='Toggle navigation')
      span.navbar-toggler-icon
        i.icon-menu
    button.topbar-toggler.more
      i.icon-options-vertical
    .nav-toggle
      button.btn.btn-toggle.toggle-sidebar
        i.icon-menu
  // End Logo Header
  // Navbar Header
  nav.navbar.navbar-header.navbar-expand-lg(data-background-color='blue2')
    .container-fluid
      #search-nav.collapse
        form.navbar-left.navbar-form.nav-search.mr-md-3
          .input-group
            .input-group-prepend
              button.btn.btn-search.pr-1(type='submit')
                i.fa.fa-search.search-icon
            input.form-control(type='text' placeholder='Search ...')
      ul.navbar-nav.topbar-nav.ml-md-auto.align-items-center
        li.nav-item.toggle-nav-search.hidden-caret
          a.nav-link(data-toggle='collapse' href='#search-nav' role='button' aria-expanded='false' aria-controls='search-nav')
            i.fa.fa-search
        li.nav-item.dropdown.hidden-caret
          a#messageDropdown.nav-link.dropdown-toggle(href='#' role='button' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
            i.fa.fa-envelope
          ul.dropdown-menu.messages-notif-box.animated.fadeIn(aria-labelledby='messageDropdown')
            li
              .dropdown-title.d-flex.justify-content-between.align-items-center
                | Messages &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;
                a.small(href='#') Mark all as read
            li
              .message-notif-scroll.scrollbar-outer
                .notif-center
                  a(href='#')
                    .notif-img
                      img(src='../assets/img/jm_denis.jpg' alt='Img Profile')
                    .notif-content
                      span.subject Jimmy Denis
                      span.block
                        | How are you ?
                      span.time 5 minutes ago
                  a(href='#')
                    .notif-img
                      img(src='../assets/img/chadengle.jpg' alt='Img Profile')
                    .notif-content
                      span.subject Chad
                      span.block
                        | Ok, Thanks !
                      span.time 12 minutes ago
                  a(href='#')
                    .notif-img
                      img(src='../assets/img/mlane.jpg' alt='Img Profile')
                    .notif-content
                      span.subject Jhon Doe
                      span.block
                        | Ready for the meeting today...
                      span.time 12 minutes ago
                  a(href='#')
                    .notif-img
                      img(src='../assets/img/talha.jpg' alt='Img Profile')
                    .notif-content
                      span.subject Talha
                      span.block
                        | Hi, Apa Kabar ?
                      span.time 17 minutes ago
            li
              a.see-all(href='javascript:void(0);')
                | See all messages
                i.fa.fa-angle-right
        li.nav-item.dropdown.hidden-caret
          a#notifDropdown.nav-link.dropdown-toggle(href='#' role='button' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
            i.fa.fa-bell
            span.notification 4
          ul.dropdown-menu.notif-box.animated.fadeIn(aria-labelledby='notifDropdown')
            li
              .dropdown-title You have 4 new notification
            li
              .notif-scroll.scrollbar-outer
                .notif-center
                  a(href='#')
                    .notif-icon.notif-primary
                      i.fa.fa-user-plus
                    .notif-content
                      span.block
                        | New user registered
                      span.time 5 minutes ago
                  a(href='#')
                    .notif-icon.notif-success
                      i.fa.fa-comment
                    .notif-content
                      span.block
                        | Rahmad commented on Admin
                      span.time 12 minutes ago
                  a(href='#')
                    .notif-img
                      img(src='../assets/img/profile2.jpg' alt='Img Profile')
                    .notif-content
                      span.block
                        | Reza send messages to you
                      span.time 12 minutes ago
                  a(href='#')
                    .notif-icon.notif-danger
                      i.fa.fa-heart
                    .notif-content
                      span.block
                        | Farrah liked Admin
                      span.time 17 minutes ago
            li
              a.see-all(href='javascript:void(0);')
                | See all notifications
                i.fa.fa-angle-right
        li.nav-item.dropdown.hidden-caret
          a.nav-link(data-toggle='dropdown' href='#' aria-expanded='false')
            i.fas.fa-layer-group
          .dropdown-menu.quick-actions.quick-actions-info.animated.fadeIn
            .quick-actions-header
              span.title.mb-1 Quick Actions
              span.subtitle.op-8 Shortcuts
            .quick-actions-scroll.scrollbar-outer
              .quick-actions-items
                .row.m-0
                  a.col-6.col-md-4.p-0(href='#')
                    .quick-actions-item
                      i.flaticon-file-1
                      span.text Generated Report
                  a.col-6.col-md-4.p-0(href='#')
                    .quick-actions-item
                      i.flaticon-database
                      span.text Create New Database
                  a.col-6.col-md-4.p-0(href='#')
                    .quick-actions-item
                      i.flaticon-pen
                      span.text Create New Post
                  a.col-6.col-md-4.p-0(href='#')
                    .quick-actions-item
                      i.flaticon-interface-1
                      span.text Create New Task
                  a.col-6.col-md-4.p-0(href='#')
                    .quick-actions-item
                      i.flaticon-list
                      span.text Completed Tasks
                  a.col-6.col-md-4.p-0(href='#')
                    .quick-actions-item
                      i.flaticon-file
                      span.text Create New Invoice
        li.nav-item.dropdown.hidden-caret
          a.dropdown-toggle.profile-pic(data-toggle='dropdown' href='#' aria-expanded='false')
            .avatar-sm
              img.avatar-img.rounded-circle(src='../assets/img/profile.jpg' alt='...')
          ul.dropdown-menu.dropdown-user.animated.fadeIn
            .dropdown-user-scroll.scrollbar-outer
              li
                .user-box
                  .avatar-lg
                    img.avatar-img.rounded(src='../assets/img/profile.jpg' alt='image profile')
                  .u-text
                    h4 Hizrian
                    p.text-muted hello@example.com
                    a.btn.btn-xs.btn-secondary.btn-sm(href='profile.html') View Profile
              li
                .dropdown-divider
                a.dropdown-item(href='#') My Profile
                a.dropdown-item(href='#') My Balance
                a.dropdown-item(href='#') Inbox
                .dropdown-divider
                a.dropdown-item(href='#') Account Setting
                .dropdown-divider
                a.dropdown-item(href='#') Logout
  // End Navbar
